                    <ul class="nav nav-tabs notika-menu-wrap menu-it-icon-pro">
                        @{
                            var menuss = (List<MainMenu>)Session["Menu"];
                            var subs = (List<SubMenu>)Session["Subs"];
                            foreach (var menu in menuss)
                            {
                                if (menu == menuss.First())
                                {
                                    <li class="active">
                                        <a data-toggle="tab" href="#@menu.MainMenuName"><i class="notika-icon notika-house"></i> @menu.MainMenuName</a>
                                    </li>
                                }
                                else
                                {
                                    <li>
                                        <a data-toggle="tab" href="#@menu.MainMenuName"><i class="notika-icon notika-house"></i> @menu.MainMenuName</a>
                                    </li>
                                }
                            }
                        }
                        <li>
                            @Html.ActionLink("Log Out", "LogOut", "Users")
                        </li>
                    </ul>
                    <div class="tab-content custom-menu-content">
                        @foreach (var menu in menuss)
                        {
                            if (menu == menuss.First())
                            {
                                <div id="@menu.MainMenuName" class="tab-pane in active notika-tab-menu-bg animated flipInX">
                                    <ul class="notika-main-menu-dropdown">
                                        @foreach (var sub in subs)
                                        {
                                            if (sub.MainMenuId == menu.MainMenuId)
                                            {
                                                <li>
                                                    @Html.ActionLink(sub.SubMenuName, sub.SubMenuName, menu.MainMenuName, null, new { @class = "ajaxclass" })
                                                </li>
                                            }
                                        }
                                    </ul>
                                </div>
                            }
                            else
                            {
                                <div id="@menu.MainMenuName" class="tab-pane notika-tab-menu-bg animated flipInX">
                                    <ul class="notika-main-menu-dropdown">
                                        @foreach (var sub in subs)
                                        {
                                            if (sub.MainMenuId == menu.MainMenuId)
                                            {
                                                <li>
                                                    @Html.ActionLink(sub.SubMenuName, sub.SubMenuName, menu.MainMenuName, null, new { @class = "ajaxclass" })
                                                </li>
                                            }
                                        }
                                    </ul>
                                </div>
                            }
