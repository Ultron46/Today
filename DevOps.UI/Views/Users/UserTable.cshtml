    <script>
        $(document).ready(function () {
            var role = '@Session["Role"].ToString()';
            var org = '@Session["Organization"].ToString()';
            var urlg = "http://localhost:60969/api/Users/GetAuthUsers?role=" + role + "&organization=" + org;
            $.ajax({
                url: urlg,
                context: document.body,
                success: function (data) {
                    console.log(data);
                    for (var i = 0, li = data.length; i < li; i++) {
                        var newTr = "<tr> <td>" + data[i].name + " </td><td>" + data[i].address + "</td><td>" + data[i].nationality + "</td><td>" + data[i].email + "</td> <td>" + data[i].phone + "</td> <td>" + data[i].dateOfBirth + "</td> <td>" + data[i].organisation.name + "</td><td>" + data[i].registrationDate + "</td><td> <input type='hidden' value=" + data[i].userId + "><a class='editajax' id='" + data[i].userId + "' href='#'>Edit</a></td><td><a class='deleteajax' id='" + data[i].userId + "' href='#'>Delete</a></td></tr>";
                      //  var newTr = "<tr> <td>" + data[i].name + " </td><td>" + data[i].address + "</td><td>" + data[i].nationality + "</td><td>" + data[i].email + "</td> <td>" + data[i].phone + "</td> <td>" + data[i].dateOfBirth + "</td> <td>" + data[i].organisationId + "</td><td>" + data[i].registrationDate + "</td><td><a href='#'>Edit</a></td><td><a href='#'>Delete</a></td></tr>";
                        $("#data-table-basic").append(newTr);
                        document.getElementsByClassName("editajax")[i].addEventListener("click", function (event) {
                            event.preventDefault();
                            //alert(event.target.id);
                            var url1 = "http://localhost:57996/Users/EditUser/" + event.target.id;
                            $.ajax({
                                url: url1,
                                type: 'GET',//type of posting the data
                                contentType: 'application/json',
                                dataType: 'html',
                                success: function (r) {
                                    $("#partialbody").empty();
                                    $("#partialbody").html(r);
                                },
                                error: function (xhr, ajaxOptions, thrownError) {
                                    //what to do in error
                                    alert(url1);
                                }
                            });

                        });
                        document.getElementsByClassName("deleteajax")[i].addEventListener("click", function (event) {
                            event.preventDefault();
                            //alert(event.target.id);
                            if (confirm("Are you Sure Want to delete?")) {
                                var id = event.target.id;
                                var url1 = "http://localhost:60969/api/Users/DeleteUser/" + id;
                                $.ajax({
                                    url: url1,
                                    type: 'POST',//type of posting the data
                                    data: '{id: "' + id + '"}',
                                    dataType: 'json',
                                    success: function (r) {
                                        var url2 = "http://localhost:57996/Users/Users"
                                        $.ajax({
                                            url: url2,
                                            type: 'GET',//type of posting the data
                                            contentType: 'application/json',
                                            dataType: 'json',
                                            success: function (r) {
                                                $("#partialbody").empty();
                                                $("#partialbody").append(r.ViewString);
                                            },
                                            error: function (xhr, ajaxOptions, thrownError) {
                                                //what to do in error
                                            }
                                        });
                                    },
                                    error: function (xhr, ajaxOptions, thrownError) {
                                        //what to do in error
                                        alert(thrownError);
                                    }
                                });
                            }

                        });
                    }
                        
                }
            });
        });

